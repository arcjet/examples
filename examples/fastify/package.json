{
  "dependencies": {
    "@arcjet/fastify": "1.0.0-beta.13",
    "@arcjet/inspect": "1.0.0-beta.13",
    "@fastify/autoload": "6.3.1",
    "@fastify/env": "5.0.2",
    "@fastify/type-provider-json-schema-to-ts": "5.0.0",
    "fastify": "5.6.0",
    "fastify-plugin": "5.0.1",
    "json-schema-to-ts": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "20.19.14",
    "fastify-cli": "7.4.0",
    "fastify-tsconfig": "3.0.0",
    "prettier": "3.6.2",
    "tsx": "4.20.5",
    "typescript": "5.9.2"
  },
  "name": "@arcjet-examples/fastify",
