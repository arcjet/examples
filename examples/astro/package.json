{
  "dependencies": {
    "@arcjet/astro": "1.0.0-beta.12",
    "@astrojs/node": "9.3.0",
    "@fontsource-variable/figtree": "5.2.8",
    "@fontsource/ibm-plex-mono": "5.2.6",
    "@oddbird/css-anchor-positioning": "0.6.1",
    "astro": "5.11.1"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.8.3"
  },
  "name": "@arcjet-examples/astro",
  "prettier": {
    "plugins": [
      "prettier-plugin-astro"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  },
  "repository": "github:arcjet/example-astro",
  "scripts": {
    "build": "astro build",
    "check": "astro check",
    "dev": "astro dev --host",
    "start": "astro preview --host"
  },
  "type": "module",
  "version": "0.0.0"
}
