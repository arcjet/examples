{
  "dependencies": {
    "@arcjet/inspect": "1.0.0-beta.13",
    "@arcjet/sveltekit": "1.0.0-beta.13",
    "@fontsource-variable/figtree": "5.2.9",
    "@fontsource/ibm-plex-mono": "5.2.6",
    "@oddbird/css-anchor-positioning": "0.6.1",
    "zod": "4.1.8"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "5.3.2",
    "@sveltejs/kit": "2.39.1",
    "@sveltejs/vite-plugin-svelte": "6.2.0",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.38.10",
    "svelte-check": "4.3.1",
    "typescript": "5.9.2",
    "vite": "7.1.7"
  },
  "name": "@arcjet-example/sveltekit",
  "overrides": {
    "@sveltejs/kit": {
      "cookie": "0.7.0"
    }
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "overrides": [
      {
        "files": "*.svelte",
        "options": {
          "parser": "svelte"
        }
      }
    ]
  },
  "repository": "github:arcjet/example-sveltekit",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "dev": "vite dev --host",
    "prepare": "svelte-kit sync || echo ''",
    "start": "vite preview --host"
  },
  "type": "module",
  "version": "0.0.0"
}
